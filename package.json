{
  "name": "vscode-aura",
  "displayName": "ASUS Aura Sync for VSCode",
  "description": "Adds ASUS Aura RGB integration to VSCode. (Unofficial, Windows Only)",
  "version": "0.0.1",
  "author": "obfuscatedgenerated",
  "publisher": "obfuscatedgenerated",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/obfuscatedgenerated/vscode-aura.git"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:vscode-aura.testAura"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-aura.testAura",
        "title": "Test connection to Aura"
      },
      {
        "command": "vscode-aura.closeAura",
        "title": "Close connection to Aura"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package --target win32-x64"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "vsce": "^2.10.0",
    "winax": "github:obfuscatedgenerated/node-activex"
  }
}
